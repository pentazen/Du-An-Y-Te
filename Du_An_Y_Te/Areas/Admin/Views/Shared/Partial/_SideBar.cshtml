
@using Du_An_Y_Te.Areas.Admin.Helpers
@using Service.EntityModel
@{
    ThanhVien thanhvien = null;
    if (Session["ThanhVien"] != null)
    {
        thanhvien = Session["ThanhVien"] as ThanhVien;
    }
}

<div class="page-sidebar pagescroll">

    <!-- MAIN MENU - START -->
    <div class="page-sidebar-wrapper" id="main-menu-wrapper">

        <!-- USER INFO - START -->
        <div class="profile-info row">

            <div class="profile-image col-xs-4">
                <a href="ui-profile.html">
                    @if (thanhvien != null && !string.IsNullOrEmpty(thanhvien.Picture))
                    {
                        <img alt="" src="@Url.Action("GetFileByUrl","TaiKhoan",new { url=thanhvien.Picture})" class="img-responsive img-circle">
                    }
                    else
                    {
                        <img alt="" src="~/Areas/Admin/data/profile/profile.jpg" class="img-responsive img-circle">
                    }
                    
                </a>
            </div>

            <div class="profile-details col-xs-8">

                <h3>
                    @if (!string.IsNullOrEmpty(thanhvien.Ten))
                    {
                        <a href="ui-profile.html">@thanhvien.Ten</a>
                    }
                    else
                    {
                        <a href="ui-profile.html">@thanhvien.TaiKhoan</a>
                    }
                

                    <!-- Available statuses: online, idle, busy, away and offline -->
                    <span class="profile-status online"></span>
                </h3>

                <p class="profile-title">@thanhvien.LoaiThanhVien.TenLoaiThanhVien</p>

            </div>

        </div>
        <!-- USER INFO - END -->



